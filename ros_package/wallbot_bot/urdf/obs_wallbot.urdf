  </transmission>

  <!-- ======================
       ros2_control tag — indicates to ros2_control that this URDF is controllable
       ====================== -->
  <ros2_control name="Wallbot_ros2_control" type="system">
    <hardware>
      <!-- GazeboSystemInterface will be used by gazebo_ros2_control plugin at runtime -->
      <plugin>gazebo_ros2_control/GazeboSystemInterface</plugin>
    </hardware>
  </ros2_control>

  <!-- ======================
       Gazebo plugin block to ensure Gazebo Classic loads the ros2_control plugin
       (this keeps compatibility with Gazebo Classic while exposing ros2_control interfaces)
       ====================== -->
  <gazebo>
    <plugin name="gazebo_ros2_control" filename="libgazebo_ros2_control.so">
      <robotNamespace>/</robotNamespace>
      <!-- If you need to pass ros2_control config file path, you can do it through plugin params
           or via controller_manager node arguments when launching -->
    </plugin>
  </gazebo>

</robot>
