  <!-- ======================
       Transmissions for prismatic joints (required for ros2_control)
       ====================== -->
  <transmission name="trans_Prismatic_Joint_Bottom_Left">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="Prismatic_Joint_Bottom_Left">
