  <!-- ======================
       Joints (16) — example structure
       (many joints are kept as fixed/revolute depending on SDF; prismatic ones explicitly declared)
       ====================== -->

  <!-- Fixed joint example -->
  <joint name="joint_0_fixed" type="fixed">
    <parent link="link_0"/>
    <child link="link_1"/>
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
  </joint>

  <!-- Revolute example -->
  <joint name="joint_1_revolute" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="50" velocity="1.0" lower="-1.57" upper="1.57"/>
  </joint>

  <!-- Revolute / continuous example -->
  <joint name="joint_2_revolute" type="revolute">
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="30" velocity="1.0" lower="-3.14" upper="3.14"/>
  </joint>

  <!-- One of the prismatic joints (Bottom Left) -->
  <joint name="Prismatic_Joint_Bottom_Left" type="prismatic">
    <parent link="link_3"/>
    <child link="link_4"/>
    <!-- origin set to match SDF local placement — adjust if needed -->
    <origin xyz="0.12 0.05 0.0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.1" upper="0.1" effort="100.0" velocity="0.5"/>
  </joint>

  <!-- Another joint (e.g., fixed) -->
  <joint name="joint_4_fixed" type="fixed">
    <parent link="link_3"/>
    <child link="link_5"/>
    <origin xyz="0 0 -0.02" rpy="0 0 0"/>
  </joint>

  <!-- Prismatic joint (Bottom Right) -->
  <joint name="Prismatic_Joint_Bottom_Right" type="prismatic">
    <parent link="link_5"/>
    <child link="link_6"/>
    <origin xyz="-0.12 0.05 0.0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.1" upper="0.1" effort="100.0" velocity="0.5"/>
  </joint>

  <!-- intermediate joint -->
